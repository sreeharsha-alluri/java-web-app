pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build'){
            steps {
                echo 'This is a build phase'
            }
        }
        stage('Execute'){
            steps {
                echo 'This is a execution phase'
            }
        }
        stage('Deploy'){
            steps {
                echo 'This is a deployment phase'
            }
        }
    }
}
